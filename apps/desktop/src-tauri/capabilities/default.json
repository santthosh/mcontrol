{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Default capabilities for the Mission Control app",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "http://localhost:8000/**"
        },
        {
          "url": "http://localhost:9099/**"
        },
        {
          "url": "https://*.mcontrol.ai/**"
        },
        {
          "url": "https://*.run.app/**"
        },
        {
          "url": "https://securetoken.googleapis.com/**"
        },
        {
          "url": "http://127.0.0.1:*/**"
        }
      ]
    },
    "shell:allow-open"
  ]
}
